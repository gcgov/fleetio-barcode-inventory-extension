{
	"name": "GCGOV Fleetio Inventory Extension",
	"description": "Generate and print barcodes for fleetio",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "wxt",
		"dev:firefox": "wxt -b firefox",
		"build": "wxt build",
		"build:firefox": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:firefox": "wxt zip -b firefox",
		"compile": "vue-tsc --noEmit",
		"postinstall": "wxt prepare",
		"theme:dev": "npx tailwindcss --postcss --input entrypoints/popup/style.pcss --output entrypoints/popup/style.css --watch",
		"theme:build": "npx tailwindcss --postcss --input entrypoints/popup/style.pcss --output entrypoints/popup/style.css --minify"
	},
	"dependencies": {
		"@types/dymo-label-framework": "^1.2.37",
		"axios": "^1.6.8",
		"bson-objectid": "^2.0.4",
		"hotkeys-js": "^3.13.7",
		"pinia": "^2.1.0",
		"pinia-plugin-persistedstate": "^3.2.1",
		"vue": "^3.4.0"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.0.1",
		"autoprefixer": "^10.4.19",
		"flowbite": "^1.6.5",
		"postcss": "^8.4.23",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.1",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.3.3",
		"vite": "^5.2.11",
		"vue-tsc": "^2.0.6",
		"wxt": "^0.17.0"
	}
}
